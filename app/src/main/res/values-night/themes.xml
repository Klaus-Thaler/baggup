<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Baggup" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/black_700</item>
        <item name="colorPrimaryVariant">@color/black_700</item>
        <item name="colorOnPrimary">@color/black_200</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/black_10</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
        <item name="android:textColorPrimary">@color/white</item>
        <item name="android:buttonTint">@color/black_200</item>
    </style>
    <style name="Theme.Baggup.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar">
        <item name="android:textColorPrimary">@color/black_10</item>
    </style>
    <style name="Theme.Baggup.PopupOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />
</resources>